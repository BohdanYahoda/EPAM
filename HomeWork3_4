class HomeWork3_4 {
  static void Main() {
    int result = 0;
    int step = 1;
    int number = int.Parse(Console.ReadLine());
    
    for (int i = 0; i < 9; i++)
    {
        int buffer = number/step%10;
        step = step*10;
        result = result + buffer;
    }
    
    Console.WriteLine(result);
  }
}
